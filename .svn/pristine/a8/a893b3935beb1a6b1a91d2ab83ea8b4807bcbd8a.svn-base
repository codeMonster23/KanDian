        <!--javascript-->
        <script>
            //头部
           $(".header .dropwrap").click(function(e){
               $(this).find("ul").slideToggle(200)
               e.stopPropagation();
           })
           $(document).click(function(){
             $(".header .dropwrap ul").slideUp(200)  
            })
            //左侧高度
            var height=$(".content").height()            
            $(".sideMenu").height(height) 
            //side展开收起
            $(".sideList .nav").click(function(){
                $(this).addClass("click").siblings().removeClass("click")
                $(this).toggleClass("toggle")
                $(this).next("ul").slideToggle()
            })
          //调用模拟下拉
        $.divselect($(".select p"),$(".select ul:not(.select2 ul)"),$(".select ul li:not(.select2 ul li)"));
        $(".tools .select p").click(function(){
            $(".lineHeight ul").hide() 
        });
        //分类下拉
        $(".handle .all").click(function(){             
            $(this).toggleClass("click")
            $(".classify").slideToggle(300)
            setTimeout(function(){height=$(".content").height()           
            $(".sideMenu").height(height)},300)
        })
        //分享框
        $(".operate .share").click(function(){
            $(".mask").show()
            $(this).find(".sharebox").show()
        })
        $(".mask").click(function(){
            $(this).hide()
            $(".sharebox").hide()
        })

        
        var detainInfo = $('.comWrap table .detail .info');
        
        $('.comWrap .detail .imgItem').mouseenter(function(){
            $(this).find('.icon').show()
        }).mouseleave(function(){
            $(this).find('.icon').hide()
        })
        
        $('.imgBox').viewer();

        detainInfo.on('click','.imgItem .icon',function () {
            $(this).parents('.imgBox').viewer();
        })
        
        // 展开
        
        detainInfo.on('click','.unfold',function(){
            $(this).removeClass('unfold').addClass('fold').html('收起');
            $(this).parents('.info').find('.p2').css('height','auto')
            // if($(this).parents('.imgBox'))
            $(this).siblings('.imgBox').css('height','auto')
        })
        // 收起
        detainInfo.on('click','.fold',function(){
            $(this).removeClass('fold').addClass('unfold').html('查看更多');
            $(this).parents('.info').find('.p2').css('height','30px')
            $(this).siblings('.imgBox').css('height','98px')
        })
        </script>
