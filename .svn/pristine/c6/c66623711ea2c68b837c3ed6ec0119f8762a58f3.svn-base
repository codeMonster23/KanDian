/*文字设置部分*/
.editBox .tools .s2 .select{ position:relative; border:1px solid #CBCBCB; height:23px; line-height:23px; background-repeat:no-repeat; background-image:url(../../images/worksManagement/toolsbar-icon3.png); cursor:pointer; margin-right:5px; }
