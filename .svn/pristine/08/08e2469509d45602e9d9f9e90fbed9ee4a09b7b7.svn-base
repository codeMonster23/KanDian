        <!--javascript-->
        <script>
            //头部
           $(".header .dropwrap").click(function(e){
               $(this).find("ul").slideToggle(200)
               e.stopPropagation();
           })
           $(document).click(function(){
             $(".header .dropwrap ul").slideUp(200)  
            })
            //左侧高度
            var height=$(".content").height()            
            $(".sideMenu").height(height) 
            //side展开收起
            $(".sideList .nav").click(function(){
                $(this).addClass("click").siblings().removeClass("click")
                $(this).toggleClass("toggle")
                $(this).next("ul").slideToggle()
            })
          //调用模拟下拉
        $.divselect($(".select p"),$(".select ul:not(.select2 ul)"),$(".select ul li:not(.select2 ul li)"));
        $(".tools .select p").click(function(){
            $(".lineHeight ul").hide() 
        });
        //分类下拉
        $(".handle .all").click(function(){             
            $(this).toggleClass("click")
            $(".classify").slideToggle(300)
            setTimeout(function(){height=$(".content").height()           
            $(".sideMenu").height(height)},300)
        })
  
        //分享框
        $(".operate .share").click(function(){
            $(".mask").show()
            $(this).find(".sharebox").show()
        })
        $(".mask").click(function(){
            $(this).hide()
            $(".sharebox").hide()
        })
        //期刊年份
        $(".years a").click(function(){
         $(this).addClass("cur").siblings().removeClass("cur")   
        })
        
        </script>
        //高级搜索
        <script>
                 (function(){
                     $(".from").live("click",function(){
                    var start = 1980; // 指定开始年份
                    var end = new Date().getFullYear(); // 获取当前年份
                     for(i=start;i<=end;i++)
                    {$(".fromlist").append("<li>"+i+"年</li>")}})})()
                </script> 
        <script>
                   (function(){
                        $(".to").live("click",function(){
                        var start = 1980; // 指定开始年份
                        var end = new Date().getFullYear(); // 获取当前年份
                        for(i=start;i<=end;i++)
                            {$(".tolist .ndy").after("<li>"+i+"年</li>")}}) 
                    })()
                </script> 
        <script>
                (function(){  
                                      
                    $(".selects .item").live("click",
                        function(){
                        $(".selects .itemlist").hide()   
                        $(this).siblings(".itemlist").show()
                        $(this).parent(".selects").css({"z-index":"20"})//ie6、7修正
                        $(this).parents(".selectwrap").css({"z-index":"10"})//ie6、7修正
                            })
                    $(".selects .itemlist").live("mouseover",
                    function(){$(this).show();$(this).parent(".selects").css({"z-index":"20"});$(this).parents(".selectwrap").css({"z-index":"10"})}).live("mouseout",function(){
                        $(this).hide()
                        $(this).parent(".selects").css({"z-index":"1"})
                        $(this).parents(".selectwrap").css({"z-index":"1"})
                        }   
                    )
                    $(".selects .itemlist li").live("mouseover",
                        function(){
                            $(this).addClass("gray")    
                            }).live("mouseout",
                        function(){
                            $(this).removeClass("gray") 
                            }
                    )
                    $(".selects .itemlist li").live("click",
                        function(){
                        var Text=$(this).text() 
                        $(this).parent(".selects .itemlist").siblings(".item").text(Text)    
                        $(this).parent(".selects .itemlist").hide()  
                        $(this).parent(".selects").css({"z-index":"1"})
                        $(this).parents(".selectwrap").css({"z-index":"1"})
                        $(this).parent(".selects .itemlist").removeClass("itemlistspec")
                        $(this).parents(".selects").removeClass("selectspec")
                        $(this).parent().siblings(".sweep").css({"display":"block"})
                        $(this).parent().siblings("input").css({"display":"block"})
                        $(this).parent().siblings(".yearwrap").css({"display":"none"})
                         $(".searchwrap .search").removeClass("bd")
                            })
                    $(".selects .itemlist .fbsj").live("click",
                        function(){
                        $(this).parent(".selects .itemlist").addClass("itemlistspec")
                        $(this).parents(".selects").addClass("selectspec")
                        $(this).parent().siblings("input").css({"display":"none"})
                        $(this).parent().siblings(".sweep").css({"display":"none"})
                        $(this).parent().siblings(".yearwrap").css({"display":"block"})
                        $(".searchwrap .search").addClass("bd")
                            })
                    })()
            </script> 
        <script>
                (function(){
                    $(".yearwrap .p").live("mouseover",
                    function(){
                            $(this).addClass("hoverp")}).live("mouseout",
                    function(){
                            $(this).removeClass("hoverp")})
                    $(".yearwrap .p").live("click",
                        function(){
                            $(".yearwrap ul").css({"display":"none"})
                            $(".yearwrap .p").removeClass("clickp")
                            $(this).parents(".selectspec").css({"z-index":100})  
                            $(this).parents(".selectwrap").css({"z-index":10})   
                            $(this).siblings("ul").show(5)
                            $(this).addClass("clickp")}//不触发body点击事件用live
                    )
                    $(".yearwrap ul li").live("mouseover",
                        function(){
                            $(this).addClass("hover")}).live("mouseout",function(){
                            $(this).removeClass("hover")})
                    $(".selects .yearwrap ul li").live("click",
                        function(){
                        var Text=$(this).text() 
                        $(this).parent("ul").siblings(".p").children("b").text(Text)    
                        $(this).parent("ul").hide() 
                        $(".selects .itemlist").removeClass("itemlistspec")
                            })      
                        $(".selects .yearwrap ul").live("mouseover",function(){
                        $(this).css({"display":"block"})}).live("mouseout",function(){
                        $(this).css({"display":"none"})
                        $(this).siblings(".p").removeClass("clickp")
                        })
                        $("body").click(
                        function(){
                        $(".selects ul").hide()
                        $(".selects").css({"z-index":"1"})
                        $(".selectwrap").css({"z-index":"1"})
                        $(".yearwrap .p").removeClass("clickp") 
                            })})()
                </script> 
