		<script>
			$(window).load(function(){
				// 轮播图
				$('.preview .display').banqh({
					box:".preview .picBox",//总框架
					pic:".preview .carousel",//大图框架
					pnum:".preview .imgWrap",//小图框架
					prev_btn:".smallPicBox .leftBtn",//小图左箭头
					next_btn:".smallPicBox .rightBtn",//小图右箭头
					prev:".picBox .leftBtn",//大图左箭头
					next:".picBox .rightBtn",//大图右箭头
					autoplay:false,//是否自动播放
					delayTime:400,//切换一张图片时间
					order:0,//当前显示的图片（从0开始）
					picdire:true,//大图滚动方向（true为水平方向滚动）
					mindire:true,//小图滚动方向（true为水平方向滚动）
					min_picnum:8,//小图显示数量
					cur_num:".display .desc .num .current",
					total_num:".display .desc .num .total",
				})
			})
			
			// 滚动条
			$(".preview .bottombar .desc .text").mCustomScrollbar({
				theme:"dark",
				callbacks:{
					onScroll:function(){
						
					}
				}
			});
			// 缩略图收起展开
			var thumbnail=$('.bottombar .thumbnail');
			$('.icon-thumbnail').click(function(){
				thumbnail.toggle();
			});
			// 底部收起展开
			var k=0;
			$('.bottombar .desc .fold').click(function(){
				if(k==0){
					$(this).addClass('unfold').removeClass('fold').html('展开');
					if(thumbnail.is(':visible')){
						thumbnail.hide();
					}
					$('.icon-thumbnail,.desc .num,.desc .text').hide();
					$('.descWrap').css('background','none');
					k=1;
				}else if(k==1){
					$(this).addClass('fold').removeClass('unfold').html('');
					$('.icon-thumbnail,.desc .num,.desc .text').show();
					thumbnail.show();
					$('.descWrap').css('background','rgba(0, 0, 0, 0.6)');
					k=0;
				}
				
			});

			$('.picBox').mouseenter(function(){
				$('.btn-zoom').show();
			});
			
			// 鼠标五秒内不操作隐藏缩放按钮
			StartPoint();
			var currentClickPosition1;//记录点击坐标
			var currentClickPosition2;
			var eventID;
			var timeout = 5*1000;  //设定超时时间
			var currentSecond = 0;
			document.addEventListener('click', function(){
				currentClickPosition2 +=1;
			})

			function CheckTime(){
				if(currentClickPosition1 == currentClickPosition2){
					currentSecond = currentSecond+1000;
					if(currentSecond>timeout){
						clearInterval(eventID);
						$('.btn-zoom').hide();
					}
				}else{
					currentSecond = 0;
					currentClickPosition1 = currentClickPosition2;
				}
			}

			function StartPoint(){
				currentClickPosition1 = 0;
				currentClickPosition2 = 0;
				eventID = setInterval(CheckTime,1000);
			}
		</script>
