@charset "utf-8";
.content .accWrap{ width: 1100px;}
.content .accWrap .ct{ background-image: url(../../images/accountManagement/icon1.png);}
.content .accWrap .section{ margin-bottom: 45px;}
.content .accWrap .section h2{ position: relative; padding-left: 15px; padding-bottom: 6px; height: 28px; line-height: 28px; font-size: 16px; background: url(../../images/accountManagement/icon2.png) no-repeat left center; border-bottom: 1px solid #E5E5E5; margin-bottom: 30px; font-weight: normal;}
.content .accWrap .section h2 span{ position: absolute; width: 121px; top: -1px; right: 134px; height: 28px; border: 1px solid #D8D8D8; border-radius: 3px; font-size: 14px; text-align:center;   }
.content .accWrap .section h2 span:after{ position: absolute;content: "";width:6px; height:8px; top: 10px; right: -6px; background: url(../../images/accountManagement/icon3.png) no-repeat;}
.content .accWrap .section h2 a{ position: absolute; top: -1px; right: 0; width: 122px; height: 28px; line-height: 28px; color: #1FAD61; font-size: 14px; background-color: #D4F6DB; text-align: center; border: 1px dashed #009A48;border-radius: 3px; }
.content .accWrap .section .card{ padding-top: 32px; padding-left: 40px; width: 272px; height: 110px; background: url(../../images/accountManagement/bg.png) no-repeat;}
.content .accWrap .section .card .item{ height: 30px; line-height: 30px;font-size: 14px; }
.content .accWrap .section .card .item label{ float: left; color: #0fa652; font-weight: bold; margin-right: 15px;}
.content .accWrap .section .card .item span{ float: left;}
.content .accWrap .section table{ width: 100%; font-size: 14px; text-align: center;}
.content .accWrap .section table th{ background-color: #1FAD61; height: 40px; border-right:1px solid #ffffff; color: #ffffff; font-weight: normal;}
.content .accWrap table .last{ border-right: none;}
.content .accWrap table td{ height: 43px;}
.content .accWrap table tr.bg td{ background-color: #F1F1F1;}
.content .accWrap table td a{ color: #1FAD61;}
