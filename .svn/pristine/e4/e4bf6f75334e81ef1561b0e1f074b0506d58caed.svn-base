    margin-right: 3px;
    background-color: #535B74;
    color: #9ea8c7;
    text-align: center;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

.main .template .slideWrap .zoom .tab a.cur {
    background-color: #ffffff;
    color: #262626;
}


