.fixFoot {
    position: fixed;
    bottom: 0px;
    height: 72px;
    filter: progid: DXImageTransform.Microsoft.gradient(enabled='true', startColorstr='#BF4C4C4C', endColorstr='#BF4C4C4C');
    width: 100%;
