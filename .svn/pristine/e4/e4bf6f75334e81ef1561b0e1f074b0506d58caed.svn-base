@charset "utf-8";
.main {
    width: 743px;
    margin: 0 auto;
    font-family: "microsoft yahei";
    position: relative;
}


/*返回首页*/

.main .return {
    position: fixed;
    top: 146px;
    left: 50%;
    margin-left: -450px;
    width: 66px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    padding-top: 36px;
    border: 1px solid #D8D8D8;
    font-size: 14px;
    color: #D8D8D8;
    background: #ffffff url(../../images/worksManagement/homeicon.png) no-repeat center 8px;
    transition: all .2s linear;
}

.main .return:hover {
    border-color: #F77225;
    background: #F77225 url(../../images/worksManagement/homeiconh.png) no-repeat center 8px;
    color: #ffffff;
}

.main .top {
    padding-top: 30px;
    margin-bottom: 12px;
    height: 32px;
}

.main .top .route {
    line-height: 32px;
    font-size: 14px;
    color: #252525;
}

.main .top .route a {
    color: #252525;
}

.main .top .lead {
    width: 78px;
    height: 32px;
    background: #FFF1ED;
    border: 1px solid #FFBAA3;
    border-radius: 3px;
    color: #FF6600;
    text-align: center;
    line-height: 32px;
    box-shadow: 0 0 2px 2px #F4E8E4;
}

.mainArea {
    background-color: #ffffff;
    box-shadow: 0 0 6px 2px #EBEBEB;
    margin-bottom: 80px;
}


/*editbox*/

.mainArea .editBox {
    min-height: 627px;
}

.mainArea .editBox .tools {
    margin: 0 24px;
    height: 35px;
    padding-top: 7px;
    border-bottom: 1px solid #E5E5E5;
}

.mainArea .editBox .pagebreak {
    display: block;
    clear: both !important;
    cursor: default !important;
    width: 100% !important;
    margin: 0;
    background: url(../../images/worksManagement/ph-jt-divide.png) no-repeat center;
    height: 16px;
    border: none 0;
}


/*标题*/

.mainArea .editBox .titInput {
    height: 75px;
    padding: 0 20px;
}

.mainArea .editBox .titInput input {
    width: 100%;
    height: 75px;
    line-height: 75px;
    font-family: "微软雅黑";
    font-size: 24px;
    text-align: center;
    color: #424242;
    border: none 0;
    background-color: transparent;
}

.mainArea .editBox .titInput .colorBlur {
    color: #797777;
}


