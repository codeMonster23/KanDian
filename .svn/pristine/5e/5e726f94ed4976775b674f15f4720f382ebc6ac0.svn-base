.operate .sharebox {
    display: none;
    position: absolute;
    width: 220px;
    height: 293px;
    background-color: #ffffff;
    border-radius: 4px;
    top: 67px;
    left: 55px;
    z-index: 100;
    cursor: default;
    padding-left: 0;
    box-shadow: 0px 0px 2px 2px rgba(100,100,100,0.1);
}
.operate .sharebox:before {
    content: "";
    position: absolute;
    width: 11px;
    height: 7px;
    top: -7px;
    right: 46px;
    background: url(../../images/worksManagement/icon19.png) no-repeat;
}

.operate .sharebox .ewm {
    padding-top: 21px; 
    /* padding-left: 21px;
    height: 178px;  */
}
.operate .sharebox p {
    line-height: 30px;
    text-align: center;
    font-size: 14px;
    background-color: #24ae61;
    color: white;
    margin: 10px 10px 10px 10px;
}
.operate .sharebox .site {
    margin-left: 10px;
    height: 30px;
    padding-left: 0;
}

.operate .sharebox .site span {
    float: left;
    width: 143px;
    height: 28px;
    line-height: 28px;
    color: #a6a6a6;
    border: 1px solid #D9D9D9;
    padding-left: 7px;
    border-right: none;
    background-color: #F1F1F1;
    font-size: 12px;
    margin: 0;
}

.operate .sharebox .site a {
    float: left;
    width: 49px;
    height: 30px;
    line-height: 30px;
    color: #ffffff !important;
    background-color: #168EDB;
    text-align: center;
    margin: 0 !important;
}

.mask {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: .5;
    filter: alpha(opacity=50);
    z-index: 2;
}


