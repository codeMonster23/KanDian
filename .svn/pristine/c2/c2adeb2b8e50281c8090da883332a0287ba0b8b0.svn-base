.btn-pink {
    background-color: #e48989;
}

.btn-blue {
    background-color: #3272da;
}

.btn-purple {
    background-color: #364f6b;
}

.btn-green {
    background-color: #17b978;
}

.btn-yellow {
    background-color: #f8ac59;
}

.rankingSetting .form-group {
    margin-bottom: 24px;
}

.rankingSetting .form-group-title {
    position: relative;
    padding-left: 13px;
    margin-bottom: 20px;
    font-size: 14px;
    font-weight: bold;
    color: #5a5a5a;
}

.rankingSetting .form-group-title::before {
    content: '';
    position: absolute;
    left: 0;
    top: 3px;
    width: 3px;
    height: 14px;
    background-color: #19aa8d;
}

.rankingSetting .form-group-subtitle {
    margin-left: 16px;
    font-weight: normal;
    font-size: 12px;
    color: #999999;
}

.rankingSetting .form-item-title {
    line-height: 32px;
    margin: 0 20px 0 15px;
    font-size: 14px;
}

.rankingSetting .form .inputText {
    width: 400px;
    height: 30px;
    padding: 0 10px;
    border: solid 1px #e6e6e6;
    font-size: 14px;
}

.rankingSetting .form label {
    display: block;
    margin-left: 20px;
    margin-top: 10px;
    font-size: 14px;
    font-weight: normal;
    color: #5a5a5a;
}

.rankingSetting .form input[type=radio] {
    display: inline-block;
    vertical-align: middle;
    margin: -2px 8px 0 8px;
}

.rankingSetting .btn-add {
    margin-left: 15px;
    border-color: #1c84c6;
    color: #1c84c6;
}

.rankingSetting .btn-add:hover {
    background-color: #1c84c6;
    color: #fff;
}

.rankingSetting table {
    margin-left: 15px;
    margin-top: 15px;
}

.rankingSetting .btn-cancel {
    border-color: #19aa8d;
    color: #19aa8d;
}

.rankingSetting .btn-cancel:hover {
    background-color: #19aa8d;
    color: #fff;
}

.rankingSetting .btn-save {
    background-color: #19aa8d;
}

.rankingSetting .btn-save:hover {
    /* background-color: #19aa8d;
    color: #fff; */
}