.sideframe .box2 .logo{ border-bottom: 0;}
.sideframe .box2 .logo label{ line-height: 38px; margin-bottom: 28px;}
.sideframe .box2 .logo .uploadlogo{ clear: both;}
.sideframe .box2 .logo .uploadlogo .bglogo{ width: 90px; height: 90px; border-radius: 50%; margin: 0 auto 18px; background: url(../../images/homepageSetting/icon17.png) no-repeat;}
.sideframe .box2 .logo .uploadlogo .btn{ display: block; margin: 0 auto; width: 90px; height: 30px; line-height: 30px; border-radius: 3px; background-color: #FF542D; text-align: center; color: #ffffff;} 
/*开始搭建*/
.sideframe .start{ height: 196px; background: url(../../images/homepageSetting/startlogo.jpg) no-repeat center 80px;} 
