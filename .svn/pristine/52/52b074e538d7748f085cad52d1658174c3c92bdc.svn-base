.sideframe .box1 .detail .addpic .p2{ line-height: 26px; color: #a6a6a6;} 
/*底部固定*/
.fixb{ position: fixed; left: 0; bottom: 0; width: 100%; height: 100px; background-color: rgba(0,0,0,0.2); font-family: "微软雅黑"; z-index: 999;}
.fixb .center{ margin: 0 443px 0 230px; padding-top: 29px; text-align: center;}
.fixb .center a{ display: inline-block; width: 134px; height: 43px; text-align: center; line-height: 43px; font-size: 16px; border-radius: 3px;}
.fixb .center .a1{ background-color: #ffffff; color: #f87323;}
.fixb .center .a3{ background-color: #F77225; color: #ffffff;}
.fixb .center .a2{ width:58px; height: 41px; line-height: 41px; border: 1px solid #FEC5A4; text-align:left;position: relative; padding-left: 74px; background: #FFF0EC url(../../images/homepageSetting/icon16.png) no-repeat 12px center; color: #f87323; margin: 0 12px;}
.fixb .center .a2 span{ display: none; top: 8px; right: -33px;position: absolute; padding: 2px; background-color: #ffffff;}
.fixb .center .a2:hover span{ display: block;}
.fixb .center .a2 span img{ display: block;}
/*确认删除模块*/
.popwrap{ width: 300px; height: 140px; background-color: #ffffff; border-radius:10px; font-family: "微软雅黑";}
.popwrap p{ text-align: center; font-size: 14px; line-height: 86px;}
.popwrap .btns{ width: 208px; margin: 0 auto;}
.popwrap .btns a{ width: 98px; text-align: center; height: 30px; line-height: 30px; border: 1px solid #F77226; border-radius: 3px; font-size:14px;}
.popwrap .btns a.a1{ color: #f87324; background-color: #ffffff;}
.popwrap .btns a.a2{ color: #ffffff; background-color: #f87324;}
/*基本信息*/
.sideframe .box2 .item{ padding-top: 14px; padding-bottom: 14px; border-bottom: 1px dashed #CCCCCC;}
.sideframe .box2 .item label{ width: 56px; line-height: 30px;}
.sideframe .box2 .item .input{ float: left; padding: 4px 8px; width: 341px; height: 20px; line-height: 20px; border: 1px solid #D8D8D8; font-size: 12px;}
.sideframe .box2 .item .textarea{ float: left; width: 349px; padding-left: 8px; height: 43px; position: relative;border: 1px solid #D8D8D8;}
.sideframe .box2 .item .textarea textarea{ resize: none;width: 349px;height: 26px; line-height: 26px; font-family: "微软雅黑"; font-size: 12px;}
