.settingWrap .banner a{ padding:9px 13px ; font-size: 14px; color: #ffffff; border-radius: 5px; background-color: rgba(0,0,0,0.4);}
.mobile{ margin-top:66px;margin-left: 55px; padding-top: 81px; padding-left: 16px; padding-right: 16px; padding-bottom: 81px; width:325px; height: 568px; background: url(../../images/homepageSetting/mobile.jpg) no-repeat center;}
