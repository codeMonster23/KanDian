/*开始搭建*/
.setup .showArea{ width: 373px; border: 1px solid #E5E5E5; margin: 0 auto;background-color:#E2E1E1;position: relative; }
.setup .showArea .bg{ position: relative;}
.setup .showArea .bg img{ display: block;}
.setup .showArea .bg::before{ content: " "; position: absolute; width: 100%; height: 100%;top: 0; left: 0; background-color: rgba(0,0,0,0.4);}
.setup .showArea .face{ position: absolute; width: 100%; left: 0; top: 0; text-align: center; color: #ffffff;}
.setup .showArea .face img{ display: block; margin: 0 auto;}
.setup .showArea .face .avatar{ margin: 15px auto 0; border-radius: 50%;  border: 3px solid #ffffff;}
.setup .showArea .face .p1{ font-size: 22px; line-height:46px;}
.setup .showArea .face .p2{ line-height: 28px; font-size: 14px;}
.setup .showArea .face .p3{ line-height: 26px; margin-bottom: 8px;}
.setup .showArea .nav{ margin-bottom: 6px;}
.setup .showArea .empty{ padding-top: 128px; text-align: center; background:#F7F7F7 url(../../images/homepageSetting/icon4.png) no-repeat 135px 30px; padding-bottom: 37px;margin-bottom: 8px;}
.setup .showArea .empty p{ line-height:28px; color: #262626;}
.setup .showArea .module{ background-color: #F7F7F7; padding: 0 10px; padding-bottom: 5px;}
.setup .showArea .module h1{ line-height: 46px; font-size: 14px; text-align: center;}
.setup .showArea .module .sort{ border-top: 1px solid #E2E2E2;}
.setup .showArea .module .sort h2{ font-weight: normal; line-height: 37px; font-size: 12px;}
.setup .showArea .module .sort .outer{ padding-left: 10px;}
.setup .showArea .module .sort .outer a{ float: left; width: 102px; height: 26px; line-height: 26px; text-align: center; color: #272727; border: 1px dashed #BBBBBB; background-color: #ffffff; margin-right: 10px; margin-bottom: 10px;}
.setup .showArea .module .sort .outer .line{ float: left; margin-left:-10px; margin-right: 10px; height: 0; width: 352px; border-top: 1px dashed #CCCCCC; margin-bottom: 10px;}
/*搭建*/
.autowidth .settingWrap{ padding-top: 6px; padding-bottom: 150px;}
.settingWrap .tips{ margin:0 6px 32px; line-height: 32px; color: #e59878; background-color: #FFEDE6; border: 1px solid #FDD6C6; padding-left: 9px; position: relative;}
.settingWrap .tips a{ color: #347CD9;}
.settingWrap .tips .close{ position: absolute; width: 18px; height: 18px; background: url(../../images/homepageSetting/icon5.png) no-repeat center; top: 7px; right: 7px;}
/*组件共同样式*/
.showArea .unit{ position: relative; background-color: #ffffff; padding: 0 14px; border: 1px solid #E5E5E5; margin-left: -1px; margin-right: -1px; margin-bottom: 11px; cursor: pointer;}
.showArea .unit:hover{ background-color: #F5FFF6; border: 1px dashed #38C646;}
.showArea .unit.click{background-color: #F5FFF6; border: 1px dashed #38C646;}
.showArea .unit .add{ display:none;position: absolute; width: 17px; height: 17px; background: url(../../images/homepageSetting/icon6.png) no-repeat; left: 50%; margin-left: -8px;}
.showArea .unit .add1{ top: -8px;}
.showArea .unit .add2{bottom: -8px;}
.showArea .unit .handle{ display:none;position: absolute; top:-1px; right: -37px;}
.showArea .unit .handle a{ display: block; width: 28px; height: 28px; margin-bottom: 4px;}
.showArea .unit .handle .up{ background: url(../../images/homepageSetting/icon7.png) no-repeat;}
.showArea .unit .handle .up:hover{ background-image: url(../../images/homepageSetting/icon7h.png);}
.showArea .unit .handle .down{ background: url(../../images/homepageSetting/icon8.png) no-repeat;}
.showArea .unit .handle .down:hover{background-image: url(../../images/homepageSetting/icon8h.png);}
.showArea .unit .handle .del{background: url(../../images/homepageSetting/icon9.png) no-repeat;}
.showArea .unit .handle .del:hover{ background-image: url(../../images/homepageSetting/icon9h.png);}
.showArea .unit.click .add{ display: block;}
.showArea .unit.click .handle{ display: block;}
.showArea .unit .picpart p{text-align: center; font-size: 14px; color: #666666;}
.showArea .unit .tit{ height:37px; line-height: 37px; font-weight: normal;}
.showArea .unit .tit span{ float: left; font-size:16px; font-weight: bold;} 
.showArea .unit .tit a{ float: right; font-size: 12px; color:#ff552d;}
.showArea .unit .picpart{ height: 194px; border-radius: 5px; background-color: #EAEAEA; margin-bottom: 5px; position: relative;}
.showArea .unit .picpart img{ border-radius: 5px;}
.showArea .unit .box .picpart p{ line-height: 194px;}
.showArea .unit .box .p1{ line-height: 35px; height: 35px; font-size: 16px;}
.showArea .unit .box .p2{ line-height: 30px; height: 30px; font-size: 14px; color: #666666;}
.showArea .unit .box .bottom{ height:40px; line-height:40px;}
.showArea .unit .bottom .lp{ font-size: 14px; color: #666666;}
.showArea .unit .bottom .num{ float: right; padding-left:18px; color: #929292; background: url(../../images/homepageSetting/icon11.png) no-repeat left center; margin-left:16px;}
.showArea .unit .bottom .like{ float: right; width: 11px; height: 40px; background: url(../../images/homepageSetting/icon10.png) no-repeat center;}
.showArea .unit .picpart img{border-radius: 5px; width: 100%; height: 100%;}
.showArea .unit .module{ display: none; position: absolute; right: -383px; width:354px; border-radius: 8px; box-shadow: 0 0 14px rgba(0,0,0,0.2);}
.showArea .unit .module.up{ top: -26px;}
.showArea .unit .module.down{ bottom: -264px;}
.showArea .unit .module::before{ position: absolute; content: " "; top: 19px; left: -9px; width: 9px; height: 13px; background: url(../../images/homepageSetting/icon18.png) no-repeat;}

/*轮播图*/
.showArea .m1{ padding-top: 16px; height: 162px;}
.showArea .m1 .picpart{ height: 159px; background: url(../../images/homepageSetting/lunbobg.png) no-repeat center; }
