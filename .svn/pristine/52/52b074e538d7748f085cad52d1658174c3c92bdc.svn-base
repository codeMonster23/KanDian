/*右侧固定边栏*/
.sideframe{ position: fixed; width: 443px; background-color: #ffffff; right: 0; top: 71px; font-family: "微软雅黑";}
.sideframe .mbox{ padding: 0 13px;} 
.sideframe .mbox h1{ font-size: 14px; text-align: center; line-height: 48px; border-bottom: 1px solid #D8D8D8;} 
/*轮播图设置*/
.sideframe .box1 .detail{ margin-top: 14px;}
