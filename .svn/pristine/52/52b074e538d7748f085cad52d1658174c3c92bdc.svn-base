.showArea .m3 .card .aside .like{height: 24px;}
/*看世界*/
.showArea .m4 .picpart::after{content:" "; position: absolute; top:50%;left:50%; transform:translate(-50%,-50%);width:47px; height:47px; background: url(../../images/homepageSetting/icon14.png) no-repeat center; }
.showArea .m4 .picpart p{ position: absolute; left: 0; top: 0; right: 0; bottom: 0; z-index: 10;}
/*赏美图*/
