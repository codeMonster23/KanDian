    padding: 0;
    text-align: center;
}

.bookShop [class^=table-popover]::before {
    content: '';
    position: absolute;
    top: -5px;
    left: 20px;
    width: 8px;
    height: 8px;
    border-top: 1px solid #cbcbcb;
    border-right: 1px solid #cbcbcb;
    background: #f8f8f8;
    transform: rotate(-45deg);
}

.bookShop [class^=table-popover] span {
