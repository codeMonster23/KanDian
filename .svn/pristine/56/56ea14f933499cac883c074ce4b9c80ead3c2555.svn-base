.bookShop [class^=table-popover] {
    display: none;
    position: absolute;
    top: 35px;
    left: 0;
    z-index: 10;
    width: 233px;
    padding: 13px 0 13px 12px;
    height: 56px;
    border: 1px solid #cbcbcb;
    background: #f8f8f8;
